{
  "expo": {
    "name": "rn-media-compressor",
    "slug": "rn-media-compressor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.suvishapalla.rnmediacompressor",
      "infoPlist": {
          "NSPhotoLibraryUsageDescription": "We need access to your photos and videos to upload them.",
          "NSPhotoLibraryAddUsageDescription": "We need to save processed media.",
          "NSCameraUsageDescription": "We need camera access to capture media.",
          "NSAppTransportSecurity": { "NSAllowsArbitraryLoads": true },
          "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.suvishapalla.rnmediacompressor",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d663181a-d151-43b7-8d7d-be4cd9b6d5b8"
      }
    },
    "owner": "suvisha"
  }
}
